{
    "version": 2,
    "builds": [
      {
        "src": "src/bot.ts",
        "use": "@vercel/node"
      },
      {
        "src": "api/healthcheck.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/healthcheck",
        "dest": "/api/healthcheck.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/src/bot.ts"
      }
    ]
  }
  